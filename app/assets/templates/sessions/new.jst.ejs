<div class="row">
  <form class="form-horizontal col-lg-4 col-lg-offset-4">
    <fieldset>
    <legend> Log In: </legend>

      <div class="form-group">
        <label for="email" class="col-lg-2 control-label">Email</label>
        <div class="col-lg-10">
          <input type="text" id="email" class="form-control" name="session[email]">
        </div>
      </div>


      <div class="form-group">
        <label for="pswd" class="col-lg-2 control-label">Password</label>
        <div class="col-lg-10">
            <input type="password" class="form-control" id="pswd" name="session[password]">
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-10 col-lg-offset-2">
          <button type="submit" class="btn btn-primary btn-sm">Submit</button>
        </div>
      </div>
    </fieldset>
  </form>
</div>
